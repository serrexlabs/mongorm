{
    "name": "serrexlabs/mongorm",
    "description": "Laravel based simple mongo db orm",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanath Samarasinghe",
            "email": "sanath@serrexlabs.com"
        }
    ],
    "require": {
        "illuminate/console": "5.*",
        "illuminate/container": "5.*",
        "illuminate/contracts": "5.*",
        "illuminate/http": "5.*",
